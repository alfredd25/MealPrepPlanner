{
  "version": 2,
  "framework": "nextjs",
  "projectSettings": {
    "framework": "nextjs",
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/.next",
    "rootDirectory": "frontend"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/$1" }
  ]
}