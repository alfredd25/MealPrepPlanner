{
  "version": 2,
  "framework": "nextjs",
  "projectSettings": {
    "framework": "nextjs",
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/.next",
    "rootDirectory": "frontend"
  },
  "routes": [
    { "src": "/signin", "dest": "/frontend/signin" },
    { "src": "/sign-in", "dest": "/frontend/sign-in" },
    { "src": "/signup", "dest": "/frontend/signup" },
    { "src": "/sign-up", "dest": "/frontend/sign-up" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}